{
  "name": "ocean-notes-lightning",
  "version": "1.0.0",
  "private": true,
  "description": "Ocean Notes app using LightningJS Blits",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "build": "vite build",
    "dev": "vite dev --port ${VITE_PORT:-3000}",
    "preview": "vite preview --port ${VITE_PORT:-3000}"
  },
  "devDependencies": {
    "vite": "5.4.8",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.11.1",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.9.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "@lightningjs/blits": "1.29.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
